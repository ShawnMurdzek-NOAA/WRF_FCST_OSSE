function gen_colormap( )
begin
        ;This procedure explicitly sets the red, green, and blue
        ;intensities for each element in a colormap array.
        ;
        return( (/ (/.000,.000,.000/), \   ; 0 (background)
                (/1.000,1.000,1.000/), \   ; 1 (foreground)
                (/.500,.200,.900/), \     ; 2
                (/.400,.000,.800/), \     ; 3
                (/.400,.200,1.000/), \      ; 4
                (/.000,.000,1.000/), \      ; 5
                (/.200,.400,1.000/), \      ; 6
                (/.400,.800,1.000/), \      ; 7
                (/.000,1.000,.400/), \      ; 8
                (/.000,.600,.000/), \      ; 9
                (/.600,1.000,.000/), \     ; 10
                (/.400,.900,.000/), \     ; 10
                (/1.000,1.000,1.000/), \   ; 11
                (/.700,.700,.000/), \     ; 10
                (/.800,.800,.000/), \     ; 12
                (/.900,.900,.000/), \     ; 12
                (/1.000,1.000,.000/), \     ; 12
                (/.700,.500,.200/), \     ; 13
                (/1.000,.600,.200/), \     ; 14
                (/1.000,.200,.000/), \     ; 15
                (/.400,.000,.000/), \     ; 16
                (/.800,.400,.400/), \     ; 17
                (/.392,.392,.392/), \     ; 18
                (/.300,.300,.300/), \     ; 19
                (/.700,.700,.700/) /) )    ; 20
end

